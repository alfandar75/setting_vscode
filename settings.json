{
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "workbench.startupEditor": "none",
  "update.showReleaseNotes": false,
  "editor.fontSize": 16,
  "editor.renderWhitespace": "none",
  "notebook.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "never",
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "git.enableSmartCommit": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.bracketSameLine": true,
  "workbench.settings.applyToAllProfiles": [
    "prettier.bracketSameLine"
  ],
  "workbench.editor.enablePreview": false,
  "workbench.colorTheme": "Mayukai Alucard",
  "editor.fontFamily": "Hack, Source Code Pro, Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "diffEditor.maxComputationTime": 0,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.CustomBrowser": "chrome",
  "liveServer.settings.fullReload": true,
  "liveServer.settings.NoBrowser": true,
  "liveServer.settings.port": 5500,
  "liveServer.settings.root": "/",
  "workbench.sideBar.location": "right",
  "todo-tree.tree.sortTagsOnlyViewAlphabetically": true,
  "todo-tree.general.tags": [
    // "general",
    "users",
    "dbase",
    // "relasi",
    // "trigger",
    // "css",
    "koleksilink",
    "TODO"
  ],
  "todo-tree.tree.hideIconsWhenGroupedByTag": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.autoSaveWorkspaceFilesOnly": true,
  "workbench.colorCustomizations": {
    "editor.findMatchBorder": "#0078D4",
    "editor.findMatchBackground": "#0078D450"
},
"todo-tree.regex.regex": "(//|#|--|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
"diffEditor.renderSideBySide": false,
"github.copilot.enable": {
  "*": false,
  "plaintext": false,
  "markdown": false,
  "scminput": false
},
"[html]": {
  "editor.defaultFormatter": "vscode.html-language-features"
}
}
